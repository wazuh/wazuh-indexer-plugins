{
  "index_patterns": [
    "wazuh-events-raw-v5*"
  ],
  "order": 1,
  "settings": {
    "plugins.index_state_management.rollover_alias": "wazuh-events-raw-v5",
    "mapping.total_fields.limit": 100,
    "mapping.nested_fields.limit": 10,
    "index": {
      "number_of_shards": "3",
      "number_of_replicas": "0",
      "auto_expand_replicas": "0-1",
      "refresh_interval": "5s",
      "query.default_field": [
        "wazuh.agent.id",
        "wazuh.agent.name",
        "wazuh.cluster.name",
        "wazuh.space.name"
      ],
      "max_docvalue_fields_search": 50
    }
  }
}

