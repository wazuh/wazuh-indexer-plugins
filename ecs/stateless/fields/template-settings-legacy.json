{
  "index_patterns": ["wazuh-alerts-5.x-*"],
  "order": 1,
  "settings": {
    "plugins.index_state_management.rollover_alias": "wazuh-alerts",
    "index": {
      "number_of_shards": "3",
      "number_of_replicas": "0",
      "auto_expand_replicas": "0-1",
      "refresh_interval": "2s",
      "query.default_field": [
        "agent.host.architecture",
        "agent.host.ip",
        "agent.id",
        "agent.name",
        "agent.version",
        "wazuh.cluster.name",
        "wazuh.cluster.node",
        "wazuh.schema.version"
      ]
    }
  }
}
