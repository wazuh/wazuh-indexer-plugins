{
  "index_patterns": ["wazuh-states-vulnerabilities-*"],
  "order": 1,
  "settings": {
    "index": {
      "codec": "best_compression",
      "number_of_shards": "1",
      "number_of_replicas": "0",
      "auto_expand_replicas": "0-1",
      "refresh_interval": "5s",
      "query.default_field": [
        "agent.host.architecture",
        "agent.host.ip",
        "agent.id",
        "agent.name",
        "agent.version",
        "package.architecture",
        "package.category",
        "package.description",
        "package.installed",
        "package.multiarch",
        "package.name",
        "package.path",
        "package.priority",
        "package.size",
        "package.source",
        "package.type",
        "package.vendor",
        "package.version",
        "vulnerability.id",
        "vulnerability.description",
        "vulnerability.severity",
        "wazuh.cluster.name",
        "wazuh.cluster.node",
        "wazuh.schema.version"
      ]
    }
  }
}
