{
  "index_patterns": ["wazuh-states-inventory-networks*"],
  "order": 1,
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "0",
      "refresh_interval": "2s",
      "query.default_field": [
        "agent.id",
        "agent.name",
        "agent.version",
        "agent.host.ip",
        "interface.name",
        "network.ip",
        "network.name",
        "wazuh.cluster.name",
        "wazuh.cluster.node"
      ]
    }
  }
}
