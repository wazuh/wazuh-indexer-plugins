{
  "index_patterns": [
    ".iocs_development_*iocs"
  ],
  "priority": 1,
  "data_stream": {},
  "template": {
    "settings": {
      "plugins.index_state_management.rollover_alias": ".cti-iocs",
      "mapping.total_fields.limit": 500,
      "mapping.nested_fields.limit": 50,
      "index": {
        "number_of_shards": "1",
        "number_of_replicas": "0",
        "auto_expand_replicas": "0-1",
        "refresh_interval": "2s",
        "query.default_field": [
          "document.confidence",
          "document.feed.name",
          "document.first_seen",
          "document.id",
          "document.last_seen",
          "document.name",
          "document.provider",
          "document.reference",
          "document.software.alias",
          "document.software.name",
          "document.software.type",
          "document.tags",
          "document.type",
          "type"
        ],
        "max_docvalue_fields_search": 200
      }
    }
  }
}
