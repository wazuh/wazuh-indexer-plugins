  "order": 1,
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "0",
      "refresh_interval": "5s",
      "query.default_field": [
        "agent.id",
        "agent.groups",
        "check.name",
        "check.id",
        "host.name",
        "host.os.type",
        "host.os.version",
        "policy.id",
        "policy.name",
        "policy.file"
      ]
    }
  }
}
