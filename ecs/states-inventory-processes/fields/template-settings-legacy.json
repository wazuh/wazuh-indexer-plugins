{
  "index_patterns": ["wazuh-states-inventory-processes*"],
  "order": 1,
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "0",
      "refresh_interval": "2s",
      "query.default_field": [
        "agent.host.architecture",
        "agent.host.ip",
        "agent.id",
        "agent.name",
        "agent.version",
        "process.args",
        "process.args_count",
        "process.command_line",
        "process.name",
        "process.parent.pid",
        "process.pid",
        "process.start",
        "process.state",
        "process.stime",
        "process.utime",
        "wazuh.cluster.name",
        "wazuh.cluster.node",
        "wazuh.schema.version"
      ]
    }
  }
}
