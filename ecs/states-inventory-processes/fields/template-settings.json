{
  "index_patterns": ["wazuh-states-inventory-processes*"],
  "priority": 1,
  "template": {
    "settings": {
      "index": {
        "number_of_shards": "1",
        "number_of_replicas": "0",
        "refresh_interval": "2s",
        "query.default_field": [
          "agent.host.architecture",
          "agent.host.ip",
          "agent.id",
          "agent.name",
          "agent.version",
          "process.args",
          "process.args_count",
          "process.command_line",
          "process.name",
          "process.parent.pid",
          "process.pid",
          "process.start",
          "process.state",
          "process.stime",
          "process.utime",
          "state.modified_at",
          "wazuh.cluster.name",
          "wazuh.cluster.node",
          "wazuh.schema.version"
        ]
      }
    }
  }
}
