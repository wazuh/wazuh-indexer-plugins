{
  "index_patterns": ["wazuh-states-fim*"],
  "order": 1,
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "0",
      "refresh_interval": "5s",
      "query.default_field": [
        "agent.id",
        "agent.groups",
        "file.name",
        "file.path",
        "file.target_path",
        "file.group",
        "file.uid",
        "file.gid"
      ]
    }
  }
}
